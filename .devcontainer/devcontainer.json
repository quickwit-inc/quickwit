{
    "name": "Quickwit",
    "image": "mcr.microsoft.com/devcontainers/rust:latest",
    "customizations": {
        "codespaces": {
            "openFiles": [
                "CONTRIBUTING.md"
            ]
        },
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer"
            ]
        }
    },
    "hostRequirements": {
      "cpus": 4,
      "memory": "16gb"
    },
    "postCreateCommand": ".devcontainer/post-create.sh"
}
